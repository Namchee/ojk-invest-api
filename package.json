{
	"name": "tefin-api",
	"version": "0.0.1",
	"description": "Indonesian fintech API",
	"keywords": [
		"api",
		"financial-technology",
		"fintech",
		"ojk"
	],
	"license": "MIT",
	"author": {
		"name": "Cristopher Namchee",
		"email": "cristophernamchee12@gmail.com"
	},
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"dev": "wrangler dev",
		"scrap": "node dist/index.js",
		"scrap:dev": "tsx index.ts",
		"scrap:build": "tsc",
		"deploy": "wrangler deploy --minify",
		"cf-typegen": "wrangler types --env-interface CloudflareBindings",
		"lint": "oxlint ./src/**/*.ts ./api/*.ts",
		"format": "oxfmt",
		"format:check": "oxfmt --check"
	},
	"dependencies": {
		"@hono/graphql-server": "^0.7.0",
		"@namchee/decora": "^1.0.5",
		"@namchee/telepon": "1.0.5",
		"@sentry/node": "^6.1.0",
		"@sentry/tracing": "^6.1.0",
		"graphql": "^16.12.0",
		"graphql-tag": "^2.12.6",
		"hono": "^4.11.7"
	},
	"devDependencies": {
		"@sentry/types": "^6.2.3",
		"@types/node": "^22.15.30",
		"oxfmt": "^0.23.0",
		"oxlint": "^1.38.0",
		"puppeteer": "^24.36.1",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3",
		"wrangler": "^4.61.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"puppeteer",
			"unrs-resolver"
		]
	}
}
