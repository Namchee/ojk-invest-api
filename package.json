  },
  "devDependencies": {
    "@namchee/eslint-config": "^3.1.5",
    "@sentry/types": "^6.2.3",
    "@types/node": "^22.15.30",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.38.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vercel": "^30.2.1"
